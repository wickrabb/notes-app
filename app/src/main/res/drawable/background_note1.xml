<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/colorStroke"
        android:bottom="0dp"
        android:left="@dimen/_2sdp"
        android:right="@dimen/_2sdp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <solid android:width="@dimen/_2sdp"
                android:color="@color/colorNoteColor2"/>
            <corners android:radius="@dimen/_10sdp">
            </corners>

        </shape>
    </item>



    <item
        android:id="@+id/colorCircle"
        android:bottom="0dp"
        android:top="@dimen/_3sdp">

        <shape android:shape="rectangle">
            <solid
                android:color="@color/colorDefaultNoteColor"/>
            <corners android:radius="@dimen/_10sdp">
            </corners>
        </shape>
    </item>
</layer-list>